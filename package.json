{
  "name": "slwebpagedownloader",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "description": "A simple tool to download web pages",
  "repository": {
    "type": "git",
    "url": "https://github.com/dayuqichengbao/slwebpagedownloader.git"
  },
  "author": "Shawn Liu",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build && electron-builder --publish never",
    "preview": "vite preview",
    "package-win": "electron-builder -w appx:x64 && electron-builder -w appx:ia32 && electron-builder -w appx:arm64",
    "package-win-ci": "electron-builder -w --x64 -p never && electron-builder -w --ia32 -p never",
    "package-mac": "electron-builder --mac --x64"
  },
  "mac": {
    "icon": "public/icon.icns"
  },
  "win": {
    "icon": "public/icon.ico"
  },
  "dependencies": {
    "@fontsource/inter": "^5.2.8",
    "@tailwindcss/vite": "^4.1.18",
    "electron-store": "^11.0.2",
    "tailwindcss": "^4.1.18",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "electron": "^30.0.1",
    "electron-builder": "^24.13.3",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vite-plugin-electron": "^0.28.6",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue-tsc": "^2.0.26"
  },
  "main": "dist-electron/main.js"
}